<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>shibayan with (Fork by Fernando Cerqueira)</Authors>
    <Company>shibayan</Company>
    <Description>Interactive command line interface toolkit for C#</Description>
    <PackageReleaseNotes>https://github.com/FRACerqueira/Sharprompt/releases ; https://github.com/shibayan/Sharprompt/releases</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/FRACerqueira/Sharprompt</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>2.4.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/FRACerqueira/Sharprompt</PackageProjectUrl>
    <PackageId>Sharprompt-FC</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>

</Project>
